{
  "name": "DEVPROJECT",
  "dockerComposeFile": "compose.dev.yml",
  "service": "DEVPROJECT",
  "workspaceFolder": "/workspace/DEVPROJECT",
  "remoteUser": "node",
  "forwardPorts": [3000, 27017],
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash",
        "editor.formatOnSave": true
      },
      "extensions": [
        "christian-kohler.npm-intellisense",
        "christian-kohler.path-intellisense",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "rvest.vs-code-prettier-eslint",
        "mongodb.mongodb-vscode",
        "jeremyrajan.mongo-snippets-for-node-js",
        "inferrinizzard.prettier-sql-vscode",
        "pkief.material-icon-theme",
        "mikestead.dotenv",
        "streetsidesoftware.code-spell-checker"
      ]
    }
  },
  "postCreateCommand": "curl -fsSL https://raw.githubusercontent.com/MaksTinyWorkshop/devcontainer-framework/main/scripts/postCreate.sh | bash"
}
